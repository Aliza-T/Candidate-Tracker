

@using ActionFilterClass.Data
@model IEnumerable<Candidate>

<table class="table table-hover table-bordered table-striped">
    <tr>
       
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Phone Number</th>
        <th>Notes</th>
    </tr>
    @foreach (Candidate c in Model)
            {
        <tr>
           
            <th>@c.FirstName</th>
            <th>@c.LastName</th>
            <th>@c.Email</th>
            <th>@c.Number</th>
            <th>@c.Notes</th>
        </tr>
    }
</table>





