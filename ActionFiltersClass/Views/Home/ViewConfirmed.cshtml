
@using ActionFilterClass.Data
@model IEnumerable<Candidate>

<button class="btn btn-danger" id="toggle">Toggle Notes cuz why not?</button>
<table class="table table-hover table-bordered table-striped">
    <tr>
        
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Phone Number</th>
        <th  class="notes">Notes</th>
    </tr>
    @foreach (Candidate c in Model)
            {
        <tr>
           
            <th>@c.FirstName</th>
            <th>@c.LastName</th>
            <th>@c.Email</th>
            <th>@c.Number</th>
            <th class="notes">@c.Notes</th>
        </tr>
    }
</table>

@section scripts
{
    <script src="~/Scripts/Details.Js"></script>
    }




